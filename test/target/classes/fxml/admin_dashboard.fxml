<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="20" alignment="CENTER" xmlns:fx="http://javafx.com/fxml" 
      fx:controller="com.onlinetest.controller.AdminDashboardController"
      stylesheets="@css/style.css"
      styleClass="dashboard-container">

    <!-- Dashboard Title -->
    <Label text="Admin Dashboard" styleClass="label" style="-fx-font-size: 20px; -fx-font-weight: bold;"/>

    <!-- Buttons -->
    <Button text="Add Question" onAction="#handleAddQuestion" styleClass="button"/>
    <Button text="Logout" onAction="#handleLogout" styleClass="button"/>
    
</VBox>
